<%- include('partials/head') %>

    <h1>
        Your random number is: <%= num %>
    </h1>
    <% if (num % 2===0) { %>
        <h2>Your number is even!</h2>
        <% } else { %>
            <h2>Your number is odd!</h2>
            <% } %>

                <h3>
                    <%= num%2===0 ? "even" : "odd" %>
                </h3>

                <%- include('partials/footer') %>